QToolBar {
    spacing: 1px; /* spacing between items in the tool bar */
	border: 0px none transparent;
	background: #f5f5f5;
	padding-left: 3px;
	border-top: 1px solid #ffffff;
}

QToolBar::handle {
	background-image: url(:/img/icon/handle.png);
	background-repeat: no-repeat;
	background-position: left center;
	width:5px;
	margin-right: 5px;
}

/* Action Buttons */
QToolButton { border-radius: 2px; }
QToolBar > QToolButton { height: 20px;  }
#tRender > QToolButton, #tAnim > QToolButton { height: 20px; width: 20px; }
QToolButton:on { border: 1px solid rgba(0, 0, 0, 32); background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.884548, fx:0.5, fy:0.5, stop:0 rgba(255, 255, 255, 0), stop:0.824885 rgba(58, 58, 58, 16), stop:1 rgba(58, 58, 58, 64)); margin: 0px; padding: 1px; }
QToolButton:!on { border: 1px solid transparent; background: #f5f5f5; margin: 0px; padding: 1px; }
QToolButton:!hover { }
QToolButton:hover { border: 1px solid rgba(0, 183, 255, 255); background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.884548, fx:0.5, fy:0.5, stop:0 rgba(255, 255, 255, 0), stop:0.818182 rgba(0, 183, 255, 64), stop:1 rgba(0, 255, 231, 128)); margin: 0px; padding: 1px; }
QToolButton:pressed { margin-top: 1px; margin-left: 1px; }

