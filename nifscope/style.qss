QDockWidget {
	background: #f5f5f5;
}

QDockWidget > QWidget { background: #f5f5f5; }

QDockWidget::close-button, QDockWidget::float-button {
    border: 0px solid transparent;
    background: transparent;
    padding: 0px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    border: 0px solid transparent;
    background: transparent;
    padding: 0px;
}

QDockWidget::close-button {

}

QDockWidget::float-button {

}


QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
}

QDockWidget::title {
	padding: 2px 0px 0px 6px;
	background: #f5f5f5;
	border: 0px solid #f5f5f5;
}

QDockWidget QAbstractScrollArea {
	margin: 0px;
	padding: 0px;
	background: #fefefe;
	padding-bottom: 1px;
}

QDockWidget QAbstractScrollArea::corner { background: transparent; border-radius: 2px; }


QScrollBar:vertical {
	margin: 0px 0px 5px 0px;
	padding: 2px 1px 2px 1px;
	border: 0px solid #FFFFFF;
	border-radius: 2px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0.44 rgba(255, 255, 255, 0), stop:0.45 rgba(200, 200, 200, 255), stop:0.55 rgba(200, 200, 200, 255), stop:0.56 rgba(255, 255, 255, 0));
	width: 12px;
}

QScrollBar:vertical:hover {
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0.44 rgba(255, 255, 255, 0), stop:0.45 rgba(0, 203, 243, 128), stop:0.55 rgba(0, 203, 243, 128), stop:0.56 rgba(255, 255, 255, 0));
}

QScrollBar:top-arrow:vertical, QScrollBar::bottom-arrow:vertical { background: transparent; }

QScrollBar::handle:vertical { background: #777777; margin: 0px 1px 0px 1px; border-radius: 4px; border: none; min-height: 25px; }

QScrollBar::handle:vertical:hover {
	background: rgba(0, 203, 243, 255);	
}

QScrollBar::add-line:vertical { 
border-style: none; border: 0px solid #FFFFFF; background: transparent; width: 0px; subcontrol-position: right; subcontrol-origin: margin; 
}
QScrollBar::sub-line:vertical {
border-style: none; border: 0px solid #FFFFFF; background: transparent; width: 0px; subcontrol-position: left; subcontrol-origin: margin;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical { background: transparent; }

QScrollBar:horizontal { 
	margin: 0px 5px 0px 0px;
	padding: 0px 2px 1px 2px;
	border: 0px solid #FFFFFF;
	border-style: none;
	border-radius: 2px;
	background: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1.0, stop:0.44 rgba(255, 255, 255, 0), stop:0.45 rgba(200, 200, 200, 255), stop:0.55 rgba(200, 200, 200, 255), stop:0.56 rgba(255, 255, 255, 0));
	height: 12px;
}

QScrollBar:horizontal:hover {
	background: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1.0, stop:0.44 rgba(255, 255, 255, 0), stop:0.45 rgba(0, 203, 243, 128), stop:0.55 rgba(0, 203, 243, 128), stop:0.56 rgba(255, 255, 255, 0));
}

QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal { background: transparent; }

QScrollBar::handle:horizontal { background: #777777; min-width: 20px; margin: 2px 0px 1px 0px; border-radius: 4px; border: none; }

QScrollBar::handle:horizontal:hover {
	background: rgba(0, 203, 243, 255);
}

QScrollBar::add-line:horizontal { 
border-style: none; border: 0px solid #FFFFFF; background: transparent; width: 0px; subcontrol-position: right; subcontrol-origin: margin; 
}
QScrollBar::sub-line:horizontal {
border-style: none; border: 0px solid #FFFFFF; background: transparent; width: 0px; subcontrol-position: left; subcontrol-origin: margin;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal { background: transparent; }


QSlider::groove:horizontal {
	border: 1px solid #999999;
	height: 8px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
	margin: 2px 0;
}

QSlider::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
	border: 1px solid #5c5c5c;
	width: 18px;
	margin: -2px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
	border-radius: 1px;
}
QSlider::groove:horizontal:disabled {
	border: 1px solid #CCCCCC;
	height: 8px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #DADADA, stop:1 #F1F1F1);
	margin: 2px 0;
}

QSlider::handle:horizontal:disabled {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #DDDDDD, stop:1 #FFFFFF);
	border: 1px solid #afafaf;
	width: 18px;
	margin: -2px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
	border-radius: 1px;
}
			
